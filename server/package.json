{
  "name": "server",
  "version": "1.0.0",
  "description": "backend for social media app",
  "main": "index.js",
  "scripts": {
    "start": "set FILES_TYPE=js && npm run build && node build/index.js",
    "dev": "set FILES_TYPE=ts && nodemon src/index.ts",
    "build": "rmdir /s /q .\\build && tsc",
    "test": "set NODE_ENV=test && db-migrate up --env test && npm run build && jasmine && db-migrate reset --env test",
    "migrate": "db-migrate up",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d ./src/ormconfig.ts",
    "migration:generate": "npm run typeorm -- -d ./src/ormconfig.ts migration:generate ./src/migrations/%config_name%",
    "migration:create": "npm run typeorm -- migration:create ./src/migrations/%config_name%",
    "migration:revert": "npm run typeorm -- -d ./src/ormconfig.ts migration:revert"
  },
  "author": "yousef hamaky",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "validatorjs": "^3.22.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/morgan": "^1.9.6",
    "@types/node": "^20.8.2",
    "@types/supertest": "^2.0.13",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
